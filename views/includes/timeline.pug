.grid-33
  .app--section
    ul.app--tweet--list
      each t in text 
        li
          strong.app--tweet--timestamp #{time}
          a.app--tweet--author
            .app--avatar(style=`background-image: url(${image})`)
            h4 #{user}
            |  @#{screenName}
          p=t
      include tweetactions.pug