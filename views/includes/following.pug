.grid-33
    .app--section
      ul.app--user--list
        each follow in follows
          li
            .circle--fluid
              .circle--fluid--cell.circle--fluid--primary
                a.app--tweet--author
                  .app--avatar(style=`background-image: url(${follow.profileImg})`)
                    img(src='static/images/f-spore.png')
                  h4=follow.name
                  p @#{follow.screenName}
              .circle--fluid--cell
                a.button.button-text Unfollow
      